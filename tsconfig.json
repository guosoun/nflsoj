{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": { "*": ["types/*"] },
        "module": "commonjs",
        "preserveConstEnums": true,
        "sourceMap": true,
        "outDir": "./models-built",
        "rootDir": "./models",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true
    },
    "include": [
        "models/**/*"
    ],
    "exclude": [
        "node_modules",
        "uploads"
    ]
}
